<h1>Member List</h1>

<%= link_to 'Create Member', new_members_path %>

<table>
  <thead>
    <tr>
      <td>Name</td>
      <td>Email</td>
      <td>Contact</td>
      <td>Action</td>
    </tr>
  </thead>
  <tbody>
    <% @members.each do |member| %>
      <tr>
        <td><%= member.name %></td>
        <td><%= member.email %></td>
        <td><%= member.contact %></td>
        <td><%= link_to 'Edit', edit_members_path(member) %></td>
      </tr>
    <% end %>
  </tbody>
</table>